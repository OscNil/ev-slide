<h1><%= @user.first_name %> <%= @user.last_name %> </h1>
<p><%= @user.email %> </p>
<p><%= @user.car_plate %> </p>

<% if @session %>
  <p>Charger: <%= @session.charging_post_id %>
  <!-- Nedan skall lösas så det sker automatiskt (JavaScript) senare. -->
  <p>Starting time: <%= @session.start_time%></p>
<% else %>
  <div class="btn btn-primary">Kamera</div>
<% end %>
